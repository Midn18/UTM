package apalab1;

import java.util.concurrent.TimeUnit;

public class ApaLab1
{

    public static void main(String[] args) throws InterruptedException
    {
    long startTime = System.nanoTime();
    System.out.println("" + fib1(100));
    long endTime = System.nanoTime();
    long timeElapsed = endTime - startTime;
    System.out.println("Timpul de executie în nanosecunde: " + timeElapsed);

    long startTime = System.nanoTime();
    System.out.println("" + fib2(100));
    long endTime = System.nanoTime();
    long timeElapsed = endTime - startTime;
    System.out.println("Timpul de executie în nanosecunde: " + timeElapsed);

    long startTime = System.nanoTime();
    System.out.println("" + fib3(100));
    long endTime = System.nanoTime();
    long timeElapsed = endTime - startTime;
    System.out.println("Timpul de executie în nanosecunde: " + timeElapsed);
}

public static long fib1(long n)
{
    if (n < 2)
    {
        return (int)n;
    }
    else
    {
        return fib1(n - 1) + fib1(n - 2);
    }

}

public static long fib2(long n)
{
    long i = 1;
    long j = 0;

    for (long k = 1; k <= n; k++)
    {
        j = i + j;
        i = j - i;
    }

    return j;
}

public static long fib3(long n)
{
    long i = 1;
    long j = 0;
    long k = 0;
    long h = 1;
    long t;

    while (n > 0)
    {
        if (n % 2 != 0)
        {
            t = j * h;
            j = i * h + j * k + t;
            i = i * k + t;
        }
        t = (long)Math.pow(h, 2);
        h = (2 * k * h) + t;
        k = (long)(Math.pow(k, 2) + t);
        n = n / 2;
    }
    return (int)j;
}
}
